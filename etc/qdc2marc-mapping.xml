<!--this mapping provides qualified duclin core to the following Marc fields mapping:   
    024 |a[dc.Identifier]
    041 |a[dc.Language]   
 
    245 |a[dc.Title]
    246 |a[dc.Title^alternative]    
    260 |b[dc.Publisher] |c[dc.Date^issued] |g[dc.Date^created]
    
    300 |a[dc.Format^extent] 
    307 |a[dc.Date^available]     
    340 |a[dc.Format^medium]

    500 |a[dc.Coverage] 
    505 |a[dc.Description^tableOfContents]
    510 |a[dc.Relation^isReferencedBy]
    513 |b[dc.Coverage^spatial]           
    518 |a[dc.Date^valid]
    520 |a[dc.Description]
    520 |a[dc.Description^abstract]
    522 |a[dc.Coverage^temporal]
    538 |a[dc.Relation^requires]
    540 |a[dc.Rights^accessRights] |b[dc.RightsHolder]
    540 |a[dc.Rights^license] |b[dc.RightsHolder]
    540 |a[dc.Rights] |b[dc.RightsHolder]
    546 |a[dc.Language]
    583 |d[dc.Date^modified] 


    650 |a[dc.Subject]
    655 |a[dc.Type]
    
    720 |a[dc.Contributor] |a[dc.Creator]
    774 |n[dc.Relation^hasPart]
    774 |n[dc.Relation^^isPartOf]
    775 |n[dc.Relation^hasVersion]
    775 |n[dc.Relation^isVersionOf]
    776 |n[dc.Relation^hasFormat]
    776 |n[dc.Relation^isFormatOf]
    780 |t[dc.Relation^replaces]
    785 |t[dc.Relation^isReplacedBy]
    786 |o[dc.Source]
    787 |o[dc.Relation]    

    856 |q[dc.Format]

 
-->

